<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:backgroundTint="#D7DBC8"
    tools:context=".fitness.settingpage.SettingPage">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#D6DAC8">

        <TextView
            android:id="@+id/TV_Setting"
            android:layout_width="147dp"
            android:layout_height="57dp"
            android:layout_marginTop="32dp"
            android:fontFamily="@font/coiny"
            android:text="Setting"
            android:textColor="#000000"
            android:textSize="34sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button_App_SP"
            android:layout_width="224dp"
            android:layout_height="54dp"
            android:layout_marginStart="93dp"
            android:layout_marginTop="83dp"
            android:layout_marginEnd="94dp"
            android:backgroundTint="@color/buttonBackgroundColorTint"
            android:fontFamily="@font/coiny"
            android:text="      App"
            android:textAlignment="textStart"
            android:textColor="#000000"
            android:textColorLink="#BC8787"
            app:icon="@drawable/ic_launcher_foreground"
            app:iconGravity="textStart"
            app:iconSize="40dp"
            app:iconTint="@color/iconTintPrimary"
            app:iconTintMode="multiply"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/TV_Setting"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/button_Account_SP"
            android:layout_width="224dp"
            android:layout_height="54dp"
            android:layout_marginTop="28dp"
            android:backgroundTint="@color/buttonBackgroundColorTint"
            android:text="         Account"
            android:textAlignment="textStart"
            android:textColor="#020000"
            android:textColorLink="#BC8787"
            app:icon="@drawable/ic_launcher_foreground"
            app:iconGravity="textStart"
            app:iconSize="30dp"
            app:iconTint="@color/iconTintPrimary"
            app:iconTintMode="multiply"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_App_SP"
            app:layout_constraintVertical_bias="0.015" />

        <Button
            android:id="@+id/button_Privacy_SP"
            android:layout_width="224dp"
            android:layout_height="54dp"
            android:layout_marginTop="28dp"
            android:backgroundTint="@color/buttonBackgroundColorTint"
            android:text="        Privacy"
            android:textAlignment="textStart"
            android:textColor="#020000"
            android:textColorLink="#BC8787"
            app:icon="@drawable/ic_launcher_foreground"
            app:iconGravity="textStart"
            app:iconSize="30dp"
            app:iconTint="@color/iconTintPrimary"
            app:iconTintMode="multiply"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_Theme_SP"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/button_Sync_SP"
            android:layout_width="224dp"
            android:layout_height="54dp"
            android:layout_marginTop="28dp"
            android:backgroundTint="@color/buttonBackgroundColorTint"
            android:text="          Sync"
            android:textAlignment="textStart"
            android:textColor="#020000"
            android:textColorLink="#BC8787"
            app:icon="@drawable/ic_launcher_foreground"
            app:iconGravity="textStart"
            app:iconSize="30dp"
            app:iconTint="@color/iconTintPrimary"
            app:iconTintMode="multiply"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_Privacy_SP"
            app:layout_constraintVertical_bias="0.017" />

        <Button
            android:id="@+id/button_Theme_SP"
            android:layout_width="224dp"
            android:layout_height="54dp"
            android:layout_marginTop="28dp"
            android:backgroundTint="@color/buttonBackgroundColorTint"
            android:text="        Theme"
            android:textAlignment="textStart"
            android:textColor="#020000"
            android:textColorLink="#BC8787"
            app:icon="@drawable/ic_launcher_foreground"
            app:iconGravity="textStart"
            app:iconSize="30dp"
            app:iconTint="@color/iconTintPrimary"
            app:iconTintMode="multiply"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_Notification_SP"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/button_Notification_SP"
            android:layout_width="224dp"
            android:layout_height="54dp"
            android:layout_marginTop="28dp"
            android:backgroundTint="@color/buttonBackgroundColorTint"
            android:text="      Notification"
            android:textAlignment="textStart"
            android:textColor="@color/black"
            android:textColorLink="#BC8787"
            app:icon="@drawable/ic_launcher_foreground"
            app:iconGravity="textStart"
            app:iconSize="30dp"
            app:iconTint="@color/iconTintPrimary"
            app:iconTintMode="multiply"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.502"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/button_Account_SP"
            app:layout_constraintVertical_bias="0.002" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>